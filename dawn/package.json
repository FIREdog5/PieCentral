{
  "name": "Dawn",
  "version": "0.6.0",
  "description": "Frontend for PIE Robotics System",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pioneers/PieCentral.git"
  },
  "main": "build/main.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "lint": "eslint .",
    "watch": "webpack --watch --progress",
    "test": "mocha --compilers js:babel-core/register --recursive renderer/**/test/*.test.js"
  },
  "betterScripts": {
    "start": {
      "command": "cp ../ansible-protos/*.proto ./build && electron build/main.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "mkdir -p ./build && cp ../ansible-protos/*.proto ./build && webpack --progress",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-preset-env": "1.3.3",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "better-npm-run": "0.0.15",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "minimist": "1.2.0",
    "mocha": "3.2.0",
    "webpack": "2.3.3"
  },
  "dependencies": {
    "brace": "0.10.0",
    "electron": "1.6.10",
    "electron-json-storage": "3.0.4",
    "immutable": "3.8.1",
    "json-loader": "0.5.4",
    "keymirror": "0.1.1",
    "lodash": "4.17.4",
    "mousetrap": "1.6.0",
    "numeral": "2.0.6",
    "object-assign": "4.1.1",
    "protobufjs": "5.0.1",
    "react": "15.5.4",
    "react-ace": "4.2.0",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.5.4",
    "react-immutable-proptypes": "2.1.0",
    "react-joyride": "1.10.1",
    "react-redux": "5.0.4",
    "redux": "3.6.0",
    "redux-saga": "0.14.6",
    "riek": "1.0.7",
    "seedrandom": "2.4.2",
    "smalltalk": "2.2.0",
    "ssh2": "0.5.4",
    "superagent": "3.4.1"
  }
}
